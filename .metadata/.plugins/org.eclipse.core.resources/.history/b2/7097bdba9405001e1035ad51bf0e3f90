package Ch13;

class Animal
{
	public void Sound()
	{
		System.out.println("소리를 냅니다.");
	}
}

class Dog extends Animal
{
	public void Sound()
	{
		System.out.println("멍멍거립니다.");
	}
}

class Cat extends Animal
{
	public void Sound()
	{
		System.out.println("야옹거립니다.");
	}
}

public class C03OverridingMain {
	public static void main(String[] args) {
		
		Dog poppi = new Dog();
		Cat navi = new Cat();
		
		poppi.Sound();
		navi.Sound();
	}

}
