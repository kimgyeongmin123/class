
public class C02SystemOut {

	public static void main(String[] args) {
		System.out.print("안녕 만고야\n");
		System.out.print("안녕 만고야\n");
		System.out.print("안녕 만고야\n");
		System.out.print("안녕 만고야\n");
		System.out.print("안녕 만고야\n");
		
		System.out.printf("%d", 5);
		System.out.printf("%d", 5);
		System.out.printf("%d", 5);
		System.out.printf("%d", 5);
		System.out.printf("%d", 5);
		
		System.out.println("나는 만고야");
		System.out.println("나는 만고야");
		System.out.println("나는 만고야");
		System.out.println("나는 만고야");
		System.out.println("나는 만고야");
	}
}
